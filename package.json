{
    "name": "fyp-build-01",
    "version": "0.1.0",
    "private": true,
    "main": "main.js",
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.3",
        "@testing-library/react": "^12.1.4",
        "@testing-library/user-event": "^13.5.0",
        "chart.js": "^3.7.1",
        "electron": "^17.2.0",
        "json-server": "^0.17.0",
        "react": "^17.0.2",
        "react-chartjs-2": "^4.0.1",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "start-app": "electron .",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "json-server": "json-server --watch data/test.json --port 5000",
        "dev": "concurrently \"BROWSER=none npm start\" \"npm run json-server\"",
        "dev-app": "concurrently \"BROWSER=none npm start\" \"npm run json-server\" \"wait-on http://localhost:3000 && electron .\"",
        "dev-app-nowait": "concurrently \"BROWSER=none npm start\" \"npm run json-server\" \"electron .\""
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@tailwindcss/forms": "^0.5.0",
        "autoprefixer": "^10.4.4",
        "concurrently": "^7.0.0",
        "postcss": "^8.4.12",
        "tailwindcss": "^3.0.23",
        "wait-on": "^6.0.1"
    }
}
